{
    "name": "brainex/php-cube",
    "description": "A minimal PHP Framework for building simple and scalable web applications",
    "type": "project",
    "version": "0.9.10",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "core/bootstrap.php"
        ]
    },
    "require": {
        "brainex/php-cube-core": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../brainex/php-cube-core",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
